  {
    "manifest_version": 3,
    "name": "Room Request Autofill",
    "version": "1.0",
    "description": "Autofill frequently used fields in UTD room request forms.",
    "permissions": ["activeTab", "storage", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "peechy.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }